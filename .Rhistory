convertData <- function(myfiles){
allrecords <- list()
num_data <- length(myfiles)
for(i in 1:num_data){
allrecords[[i]] <- myfiles[[i]] %>%
records() %>%
bind_rows() %>%
arrange(timestamp)
}
return(allrecords)
}
